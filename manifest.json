{
  "manifest_version": 3,
  "name": "Misinformation Slayer",
  "version": "1.0",
  "description": "Parses webpage data and then slashes out anything that is demmed as bad",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
